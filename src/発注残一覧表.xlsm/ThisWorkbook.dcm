    End If
    DateA = CDate(strYY & "/" & strMM & "/" & "01")
    DateA = DateA - 1
    Range("V5") = Format(DateA, "yyyymmdd")
    
    Worksheets("édì¸êÊ").Select
    Call Clear_Head
    Call Clear_Sht
    
    Worksheets("íSìñé“").Select
    Call Clear_Head2
    Call Clear_Sht2
    Call BUMON_Get
    
    'éxìXëIëèàóù
    strCP = CP_NAME
    If Left(strCP, 2) = "OS" Or Left(strCP, 2) = "FU" Or Left(strCP, 2) = "NG" Then
        Worksheets("íSìñé“").Range("R1") = 1
    ElseIf Left(strCP, 2) = "TK" Or Left(strCP, 1) = "S" Or Left(strCP, 2) = "AK" Then
        Worksheets("íSìñé“").Range("R1") = 2
    ElseIf Left(strCP, 2) = "HB" Then
